        "foregroundImage": "./App/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.seveneleven.allo",
      "versionCode": 2,
	  "permissions": []
    },
    "web": {
      "favicon": "./App/assets/favicon.png"
    }
  }
}
