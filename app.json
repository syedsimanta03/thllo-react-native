{
  "expo": {
    "name": "Thallo",
    "slug": "Thallo",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./App/assets/icon.png",
    "splash": {
      "image": "./App/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fd7833"
    },
    "updates": {
